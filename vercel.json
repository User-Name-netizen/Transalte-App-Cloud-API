{
  "version": 2,
  "name": "transalte-app-cloud-api",
  "functions": {
    "src/server.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    },
    {
      "src": "/",
      "dest": "src/server.js"
    }
  ],
  "env": {
    "GOOGLE_CLOUD_API_KEY": "@google_cloud_api_key"
  }
}
